{
  "manifest_version": 3,
  "name": "Test Browser Extension",
  "version": "1.0.0",
  "description": "A simple test browser extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Test Browser Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://*/*"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://*/*"]
    }
  ],
  "author": "Jeff-411",
  "homepage_url": "https://github.com/Jeff-411/test-browser-extension"
}
